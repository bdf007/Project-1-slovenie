#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/bdf00/Documents/WCS/projet/projet 1/Project-1-slovenie/node_modules/.pnpm/w3c-html-validator@1.0.3/node_modules/w3c-html-validator/bin/node_modules:/mnt/c/Users/bdf00/Documents/WCS/projet/projet 1/Project-1-slovenie/node_modules/.pnpm/w3c-html-validator@1.0.3/node_modules/w3c-html-validator/node_modules:/mnt/c/Users/bdf00/Documents/WCS/projet/projet 1/Project-1-slovenie/node_modules/.pnpm/w3c-html-validator@1.0.3/node_modules:/mnt/c/Users/bdf00/Documents/WCS/projet/projet 1/Project-1-slovenie/node_modules/.pnpm/node_modules:/mnt/c/Users/bdf00/Documents/WCS/projet/projet 1/Project-1-slovenie/node_modules:/mnt/c/Users/bdf00/Documents/WCS/projet/projet 1/node_modules:/mnt/c/Users/bdf00/Documents/WCS/projet/node_modules:/mnt/c/Users/bdf00/Documents/WCS/node_modules:/mnt/c/Users/bdf00/Documents/node_modules:/mnt/c/Users/bdf00/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/bdf00/Documents/WCS/projet/projet 1/Project-1-slovenie/node_modules/.pnpm/w3c-html-validator@1.0.3/node_modules/w3c-html-validator/bin/node_modules:/mnt/c/Users/bdf00/Documents/WCS/projet/projet 1/Project-1-slovenie/node_modules/.pnpm/w3c-html-validator@1.0.3/node_modules/w3c-html-validator/node_modules:/mnt/c/Users/bdf00/Documents/WCS/projet/projet 1/Project-1-slovenie/node_modules/.pnpm/w3c-html-validator@1.0.3/node_modules:/mnt/c/Users/bdf00/Documents/WCS/projet/projet 1/Project-1-slovenie/node_modules/.pnpm/node_modules:/mnt/c/Users/bdf00/Documents/WCS/projet/projet 1/Project-1-slovenie/node_modules:/mnt/c/Users/bdf00/Documents/WCS/projet/projet 1/node_modules:/mnt/c/Users/bdf00/Documents/WCS/projet/node_modules:/mnt/c/Users/bdf00/Documents/WCS/node_modules:/mnt/c/Users/bdf00/Documents/node_modules:/mnt/c/Users/bdf00/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli.js" "$@"
else
  exec node  "$basedir/../../bin/cli.js" "$@"
fi
