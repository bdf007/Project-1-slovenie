#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/bdf00/Documents/WCS/projet/projet 1/Project-1-slovenie/node_modules/.pnpm/mime@2.6.0/node_modules/mime/node_modules:/mnt/c/Users/bdf00/Documents/WCS/projet/projet 1/Project-1-slovenie/node_modules/.pnpm/mime@2.6.0/node_modules:/mnt/c/Users/bdf00/Documents/WCS/projet/projet 1/Project-1-slovenie/node_modules/.pnpm/node_modules:/mnt/c/Users/bdf00/Documents/WCS/projet/projet 1/Project-1-slovenie/node_modules:/mnt/c/Users/bdf00/Documents/WCS/projet/projet 1/node_modules:/mnt/c/Users/bdf00/Documents/WCS/projet/node_modules:/mnt/c/Users/bdf00/Documents/WCS/node_modules:/mnt/c/Users/bdf00/Documents/node_modules:/mnt/c/Users/bdf00/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/bdf00/Documents/WCS/projet/projet 1/Project-1-slovenie/node_modules/.pnpm/node_modules/mime/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/bdf00/Documents/WCS/projet/projet 1/Project-1-slovenie/node_modules/.pnpm/mime@2.6.0/node_modules/mime/node_modules:/mnt/c/Users/bdf00/Documents/WCS/projet/projet 1/Project-1-slovenie/node_modules/.pnpm/mime@2.6.0/node_modules:/mnt/c/Users/bdf00/Documents/WCS/projet/projet 1/Project-1-slovenie/node_modules/.pnpm/node_modules:/mnt/c/Users/bdf00/Documents/WCS/projet/projet 1/Project-1-slovenie/node_modules:/mnt/c/Users/bdf00/Documents/WCS/projet/projet 1/node_modules:/mnt/c/Users/bdf00/Documents/WCS/projet/node_modules:/mnt/c/Users/bdf00/Documents/WCS/node_modules:/mnt/c/Users/bdf00/Documents/node_modules:/mnt/c/Users/bdf00/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/bdf00/Documents/WCS/projet/projet 1/Project-1-slovenie/node_modules/.pnpm/node_modules/mime/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mime/cli.js" "$@"
else
  exec node  "$basedir/../mime/cli.js" "$@"
fi
